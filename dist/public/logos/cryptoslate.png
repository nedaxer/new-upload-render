                        { id: 'hypelab-rectangle', name: 'Rectangle', tracked: false, loaded: false },
                        { id: 'hypelab-sidebar-rectangle', name: 'Sidebar Rectangle', tracked: false, loaded: false },
                        { id: 'hypelab-top-sticky', name: 'Top Sticky', tracked: false, loaded: false },
                        { id: 'hypelab-header-unit', name: 'Header Unit', tracked: false, loaded: false },
                    ];

                    elements.forEach((element) => {
                        const el = document.getElementById(element.id);
                        if (el) {
                            el.addEventListener('ready', function () {
                                if (typeof gtag !== 'undefined') {
                                    gtag('event', 'hypeLabAdReady', {
                                        'event_category': 'Ad',
                                        'event_label': element.name,
                                    });
                                }
                                element.loaded = true;
                            });

                            el.addEventListener('error', function () {
                                if (typeof gtag !== 'undefined') {
                                    gtag('event', 'hypeLabAdError', {
                                        'event_category': 'Ad',
                                        'event_label': element.name,
                                    });
                                }
                            });

                            const isInView = function (el) {
                                const rect = el.getBoundingClientRect();
                                return (
                                    rect.top >= 0 &&
                                    rect.left >= 0 &&
                                    rect.bottom <= (window.innerHeight || document.documentElement.clientHeight) &&
                                    rect.right <= (window.innerWidth || document.documentElement.clientWidth)
                                );
                            };

                            const trackInView = function () {
                                if (
                                    element.loaded &&
                                    !element.tracked &&
                                    typeof gtag !== 'undefined' &&
                                    isInView(el)
                                ) {
                                    gtag('event', 'hypeLabAdInView', {
                                        'event_category': 'Ad',
                                        'event_label': element.name,
                                    });
                                    element.tracked = true;
                                }
                            };

                            window.addEventListener('scroll', trackInView);
                            trackInView();
                        }
                    });
                }
            })();
        </script>

        
