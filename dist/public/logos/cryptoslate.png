                function cryptoLoadHypeLab () {
                    if (typeof HypeLab === 'undefined') {
                        if (attempts < 40) {
                            attempts++;
                            setTimeout(cryptoLoadHypeLab, 500);
                            return;
                        }

                        // Bug out here if we've tried too many times...
                        console.warn('Unable to load HypeLab...');
                        return;
